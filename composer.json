{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.16",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/orm": "^3.5",
        "symfony/console": "7.3.*",
        "symfony/dependency-injection": "^7.3",
        "symfony/dotenv": "7.3.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/yaml": "7.3.*",
        "uzunov-labs/redis-service": "1.0.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.87",
        "phpstan/phpstan": "^2.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Rangelcho1990/redis-toolkit"
        }
    ],
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CSL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CSL\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "db-migrate:next": "php bin/console doctrine:migrations:migrate next",
        "db-migrate:generate": "php bin/console doctrine:migrations:generate",
        "phpstan": "vendor/bin/phpstan analyse",
        "cs-fix": "vendor/bin/php-cs-fixer fix",
        "code-fix": [
            "@cs-fix",
            "@phpstan"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.3.*",
            "docker": false
        }
    }
}
